CREATE TABLE LIGUE(
   idLigue INT PRIMARY KEY,
   nom VARCHAR(50)
);

CREATE TABLE EMPLOYE(
   idLigue INT REFERENCES LIGUE(idLigue) NOT NULL,
   idEmploye INT NOT NULL,
   PRIMARY KEY(idLigue, idEmploye),
   nom VARCHAR(50),
   prenom VARCHAR(50),
   password VARCHAR(50),
   mail VARCHAR(50),
   dateDepart DATE,
   dateArrivee VARCHAR(50),
   estAdmin VARCHAR(50),
   estRoot VARCHAR(50)
);
